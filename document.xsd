<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
targetNamespace="https://www.w3schools.com"
xmlns=""
elementFormDefault="qualified">
<xs:element name="BBTrack">
  <xs:complexType>
    <xs:sequence>
    	#Paciente
		<xs:element name="Paciente">
  			<xs:complexType>
    			<xs:sequence>
    				<xs:attribute name="IdPaciente" type="xs:string" use="required"/>
    					<xs:simpleType>
    						<xs:restriction base="xs:string">
								<xs:maxLength value="45"/>
    						</xs:restriction>
  						</xs:simpleType>
					  <xs:element name="NUSS" type="xs:string" use="required"/>
						  <xs:simpleType>
    						<xs:restriction base="xs:string">
      							<xs:maxLength value="45"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Nombre" type="xs:string" use="required"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
      							<xs:length value="50"/>
      							<xs:whiteSpace value="preserve"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Apellidos" type="xs:string" use="required"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
      						<xs:length value="45"/>
      						<xs:whiteSpace value="preserve"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Sexo" type="xs:string" use="required"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
      							<xs:pattern value="hombre|mujer"/>
						    </xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Fecha_nacimiento" type="xs:date" use="required"/>
			      <xs:element name="Domicilio" type="xs:string" use="required"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
    							<xs:length value="45"/>
      						<xs:whiteSpace value="preserve"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Localidad" type="xs:string" use="required"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
    							<xs:length value="45"/>
      						<xs:whiteSpace value="preserve"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Correo_electronico" type="xs:string" use="optional"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
      							<xs:length value="45"/>
    						</xs:restriction>
  						</xs:simpleType>
			      <xs:element name="Telefono" type="xs:string" use="optional"/>
			      	<xs:simpleType>
    						<xs:restriction base="xs:string">
      							<xs:length value="45"/>
    						</xs:restriction>
  						</xs:simpleType>
            <xs:element name="Embarazada" type="xs:boolean" use="required"/>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:pattern value="Si|No"/>
                </xs:restriction>
              </xs:simpleType>

			      #Historial
    				<xs:element name="Historial">
  						<xs:complexType>
    						<xs:sequence>
    							<xs:attribute name="IdHistorial" type="xs:string" use="required"/>
    								<xs:simpleType>
    									<xs:restriction base="xs:string">
      										<xs:length value="45"/>
    									</xs:restriction>
  									</xs:simpleType>
      						<xs:element name="Fecha_primera_consulta" type="xs:string" use="required"/>
								  <xs:element name="Vacunas" type="xs:date" use="required"/>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                          <xs:length value="45"/>
                      </xs:restriction>
                    </xs:simpleType>
                  <xs:element name="Factor_de_riesgo_prenatal">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="Antecedentes_familiares" type="xs:string" use="optional"/>
                        <xs:element name="Factores_psicosociales" type="xs:string" use="optional"/>
                        <xs:element name="Antecedentes_obstreticos" type="xs:string" use="optional"/>
                        <xs:element name="Antecedentes_personales" type="xs:string" use="optional"/>
                        <xs:element name="Patologia_materna" type="xs:string" use="optional"/>
                        <xs:element name="Riesgos_especificos" type="xs:string" use="optional"/>
                        <xs:element name="Exposicion_a_teratogenos" type="xs:string" use="optional"/>
								      </xs:sequence>
                    </xs:complexType>
                #Informe			      	
    							<xs:element name="Informe">
  									<xs:complexType>
    									<xs:sequence>
    										<xs:attribute name="IdInforme" type="xs:string" use="required"/>
    											<xs:simpleType>
    												<xs:restriction base="xs:string">
      													<xs:length value="45"/>
    												</xs:restriction>
  												</xs:simpleType>
    										<xs:element name="Estado_paciente" type="xs:date" use="required"/>
    											<xs:simpleType>
    												<xs:restriction base="xs:string">
      													<xs:length value="45"/>
      													<xs:whiteSpace value="preserve"/>
    												</xs:restriction>
  												</xs:simpleType>
			      							<xs:element name="Diagnostico" type="xs:string" use="required"/>
			      								<xs:simpleType>
    												<xs:restriction base="xs:string">
      													<xs:length value="255"/>
      													<xs:whiteSpace value="preserve"/>
    												</xs:restriction>
  												</xs:simpleType>
      										
											#Pruebas
    										<xs:element name="Prueba" maxOccurs="unbounded">
  												<xs:complexType>
    												<xs:sequence>
    													<xs:attribute name="IdPrueba" type="xs:string" use="required"/>
    														<xs:simpleType>
    															<xs:restriction base="xs:string">
      																<xs:length value="45"/>
    															</xs:restriction>
  															</xs:simpleType>
      													<xs:element name="Procedencia" type="xs:string" use="optional"/>
												      	<xs:element name="Fecha_solicitud" type="xs:date" use="optional"/>
												      	<xs:element name="Fecha_prueba" type="xs:date" use="optional"/>
												      	<xs:element name="Tecnico" type="xs:string" use="optional"/>
                                <xs:element name="Observaciones" type="xs:string" use="required"/>
												      	<xs:element name="Id_externo" type="xs:string" use="required"/>
												      	<xs:element name="Tipo" type="string" use="required"/>
												      		<xs:simpleType>
															    <xs:restriction base="xs:string">
															      <xs:enumeration value="Analisis de sangre"/>
															      <xs:enumeration value="Ecografia"/>
															      <xs:enumeration value="Amniocentesis"/>
															      <xs:enumeration value="cariotipo"/>
															    </xs:restriction>
															  </xs:simpleType>
      												</xs:sequence>
  												</xs:complexType>
											</xs:element>

    									</xs:sequence>
  									</xs:complexType>
								</xs:element>

    						</xs:sequence>
  						</xs:complexType>
					</xs:element>
     
    			</xs:sequence>
  			</xs:complexType>
		</xs:element>
		#Medico
		<xs:element name="Medico">
  			<xs:complexType>
    			<xs:sequence>
      				<xs:attribute name="Licencia" type="xs:string" use="required"/>
	      			<xs:element name="Nombre" type="xs:string" use="required"/>
	      				<xs:simpleType>
							    <xs:restriction base="xs:string">
								    <xs:length value="45"/>
							    </xs:restriction>
						    </xs:simpleType>
			      	<xs:element name="Apellidos" type="xs:string" use="required"/>
			      		<xs:simpleType>
							    <xs:restriction base="xs:string">
								    <xs:length value="45"/>
								    <xs:whiteSpace value="preserve"/>
							    </xs:restriction>
						    </xs:simpleType>
			      	<xs:element name="Sexo" type="xs:string" use="required"/>
			      		<xs:simpleType>
    						  <xs:restriction base="xs:string">
      							<xs:pattern value="hombre|mujer"/>
						      </xs:restriction>
  						  </xs:simpleType>
			      	<xs:element name="Especialidad" type="xs:string" use="required"/>
			      		<xs:simpleType>
					    	  <xs:restriction base="xs:string">
					      		<xs:length value="45"/>
					    	  </xs:restriction>
					  	  </xs:simpleType>
			      	<xs:element name="Cargo" type="xs:string" use="optional"/>
    			</xs:sequence>
  			</xs:complexType>
		</xs:element>
    </xs:sequence>
  </xs:complexType>
</xs:element>
</xs:schema>